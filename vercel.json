{
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [{ "key": "Content-Type", "value": "application/javascript; charset=utf-8" }]
    },
    {
      "source": "/(.*)\\.mjs",
      "headers": [{ "key": "Content-Type", "value": "application/javascript; charset=utf-8" }]
    }
  ]
}